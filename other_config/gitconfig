[alias]
	logger = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
	mer = merge --no-ff
	co = checkout
	ca = commit -a
	st  = status
	br = branch
	bra = branch -a
	pso = push origin
	plo = pull --no-ff origin
	ft = fetch
	ftp = fetch -p origin
